{
  "name": "bsync",
  "description": "Bookmarks Sync",
  "version": "1.0",
  "manifest_version": 2,
  "minimum_chrome_version": "69",
  "background": {
    "scripts": ["aws-sdk.js", "bsync.js"],
    "persistent": false
  },
  "permissions": [
    "bookmarks",
    "storage",
    "alarms"
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "icons": {
    "16": "invaders.png",
    "32": "invaders.png",
    "48": "invaders.png",
    "128": "invaders.png"
  }
}
