{
  "name": "bsync",
  "description": "Bookmarks Sync",
  "version": "0.0.2",
  "manifest_version": 2,
  "minimum_chrome_version": "69",
  "background": {
    "scripts": ["aws-sdk.js", "bsync.js"],
    "persistent": false
  },
  "permissions": [
    "bookmarks",
    "storage",
    "alarms"
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "browser_action": {
    "default_title": "bsync",
    "default_icon": "bsync.png"
  },
  "icons": {
    "16": "bsync.png",
    "32": "bsync.png",
    "48": "bsync.png",
    "128": "bsync.png"
  }
}
